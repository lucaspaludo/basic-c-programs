#include <stdio.h>
  
  /*8.3) Escreva um programa que lê 5 números inteiros. Após a captura dos 5 números mostrar os todos os 5 números na tela um ao lado do outro separados por espaços. O programa deve imprimir na linha abaixo o maior dos números com a seguinte frase: “O maior dos números acima é: <numero>”.*/
  
  int main(void) {
  int valor[5];

  for(int i = 0; i<5; i++){
    printf("Digite um número: ");
    scanf("%i",& valor[i]);
   
    } 
  printf("\n%i %i %i %i %i", valor[0], valor[1], valor[2], valor[3],valor[4]);
  if(valor[0]>valor[1] && valor[0]>valor[2] && valor[0]>valor[3] && valor[0]>valor[4]){
    printf("\nO maior dos números acima é: %i", valor[0]);
  } else if (valor[1]>valor[0] && valor[1]>valor[2] && valor[1]>valor[3] && valor[1]>valor[4]){
    printf("\nO maior dos números acima é: %i", valor[1]);
  } else if (valor[2]>valor[0] && valor[2]>valor[1] && valor[2]>valor[3] && valor[2]>valor[4]){
    printf("\nO maior dos números acima é: %i", valor[2]);
  } else if (valor[3]>valor[0] && valor[3]>valor[1] && valor[3]>valor[2] && valor[3]>valor[4]){
    printf("\nO maior dos números acima é: %i", valor[3]);
  } else {
    printf("\nO maior dos números acima é: %i", valor[4]);
  }

  
  return 0;
}
