#include <stdio.h>

int main(void) {

  /*Dado um número inteiro positivo n, imprimir os n primeiros naturais
  ímpares. Exemplo: Para n=4 a saída deverá ser 1,3,5,7.*/

  int impar=1 , i=1, n;
  printf (" Quantidade de impares : "); scanf (" %d", &n);
  
  
  while (i <= n){
    printf (" %d\n", impar );
    impar += 2; 
    i ++; }


  return 0;
}
