#include <stdio.h>

int main(void) {
  
  /*Faça um programa que lê dois números inteiros do teclado e testa se o primeiro é
  menor que o segundo. Em caso afirmativo imprima todos os números do intervalo entre
  eles.*/


  int num1,num2;

  printf("Digite o primeiro número: "); scanf("%i", & num1);
  printf("Digite o segundo número: "); scanf("%i", & num2);

 
  if(num1<num2){
    printf("\nIrei apresentar o intervalo entre esses números:");
  } else {
    printf("Erro! Informação inválida");
  }

  for(int x = num1+1; x>num1 && x<num2; x++)

  printf("\n%i", x);
  
  return 0;
}
