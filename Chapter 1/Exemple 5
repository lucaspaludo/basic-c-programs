  printf("Número 2: "); scanf("%i", &b);
  printf("Número 3: "); scanf("%i", &c);
  
  if(a<b){
  if(a<c){ 
  printf("O menor número é: %i", a);
  } else {
  printf("O menor número é: %i", c);
  }
  } else {
  if(b<c){
  printf("O menor número é: %i", b);
  } else {
  printf("O menor número é: %i", c);
  }
  }


  





