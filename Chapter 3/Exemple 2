#include <stdio.h>
#include <math.h>

/*7.11) Escreva uma função que calcule e retorne a distância entre dois pontos (x1, y1) e (x2, y2). Todos os números e valores de retorno devem ser do tipo float.*/



float distancia_entre_os_pontos(float x1, float y1, float x2, float y2){
return sqrt((pow(x2-x1,2))+(pow(y2-y1,2)));
}

int main(void) {

printf("\nA distância entre os pontos é: %.2f",distancia_entre_os_pontos(1,4,9,-2));


  return 0;
}
